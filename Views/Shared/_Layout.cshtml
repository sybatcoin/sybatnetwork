                        <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Sybat Core</a></li>
                        <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Sybat Console</a></li>
                        <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Sybatcoin RPC</a></li>
